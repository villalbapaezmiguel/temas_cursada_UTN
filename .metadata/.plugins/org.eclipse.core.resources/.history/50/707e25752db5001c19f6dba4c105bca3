#include "Funciones.h"

/*si es par devuelve 1 de lo contrario retorna -1*/
int esPar(int numero )
{
	int estado = -1;

	if(numero %2 == 0)
	{
		estado = 1;
	}

	return estado ;
}

/*si es par devuelve 1 de lo contrario retorna -1*/
int esImpar(int numero )
{
	int estado = -1;

	if(numero %2 == 0)
	{
		estado = 1;
	}

	return estado ;
}
